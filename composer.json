{
    "name": "bradmkjr/monolog-wordpress",
    "description": "A handler for Monolog that sends messages to MySQL via WordPress Functions",
    "keywords": ["wordpress", "log", "logging", "monolog", "mysql", "database"],
    "homepage": "https://github.com/bradmkjr/monolog-wordpress",
    "license": "MIT",
    "version": "3.0.0",
    "authors": [
        {
            "name": "Bradford Knowlton",
            "email": "brad@ucanbonline.net"
        },
        {
            "name": "Bence Szalai",
            "email": "bence@sbnc.eu"
        }
    ],
    "repositories": [
        {
            "type":"svn",
            "url":"https://develop.svn.wordpress.org/"
        }
    ],
    "require": {
        "php": ">=8.1",
        "monolog/monolog": "^3.0.0"
    },
    "require-dev": {
        "wordpress/wordpress": "5 - 6",
        "phpunit/phpunit": "5.0.*"
    },
    "autoload": {
        "psr-4": {"WordPressHandler\\": "src/WordPressHandler"}
    }
}
